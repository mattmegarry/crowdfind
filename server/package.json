{
  "name": "server",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "npm run prod",
    "server:prod": "node ./dist/bin/www",
    "dev": "NODE_ENV=development babel-watch ./src/bin/www",
    "clean": "rimraf dist",
    "build": "babel ./src --out-dir dist --copy-files",
    "prod": "NODE_ENV=production npm run && npm run clean && npm run build && npm run server:prod"
  },
  "dependencies": {
    "camelcase-keys": "^6.2.2",
    "cookie-parser": "~1.4.3",
    "debug": "~2.6.9",
    "dotenv": "^8.2.0",
    "express": "~4.17.3",
    "make-runnable": "^1.3.6",
    "morgan": "~1.9.0",
    "pg-promise": "^10.5.6"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.7",
    "@babel/node": "^7.8.7",
    "@babel/preset-env": "^7.8.7",
    "babel-watch": "^7.0.0",
    "rimraf": "^3.0.2"
  }
}
